{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Note attached to holdings",
  "HoldingsNote": {
    "type": "object",
    "properties": {
      "note": {
        "type": "string",
        "description": "Text content of the note"
      },
      "holdingsNoteTypeName": {
        "type": "string",
        "description": "Name of the holdings note type"
      }
    },
    "xml": {
      "name": "notes"
    }
  }
}
